  <div id="ext-editors-hash-area2" class="layout1-area2"></div>
  <div id="ext-editors-hash-area3" class="layout1-area3"></div>
  <div id="ext-editors-hash-area4" class="layout1-area4"></div>
  <div class="dn">
    [#/ext/share/dialog/filesystem-open.ht]
    [#/ext/share/dialog/filesystem-select.ht]
  </div>
</div>
