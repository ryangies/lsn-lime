[#:js src=`./local.js`]
[#:css src=`./local.css`]
[#:js]
  var screen = new js.ext.editors.hash.Screen([#:json ./desktop.hf]);
  env.screens.add(screen);
[#:end js]
[#:css]
[#:end css]
<div id="ext-editors-hash-screen" class="screen-area screen-hidden">
